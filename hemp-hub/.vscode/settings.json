{
  "css.customData": [
    ".vscode/css_custom_data.json"
  ],
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "css.lint.unknownAtRules": "ignore",
  "css.lint.unknownProperties": "ignore",
  "tailwindCSS.validate": true,
  "tailwindCSS.includeLanguages": {
    "plaintext": "html"
  }
}
